dths><column index="1" value="67"/><column index="2" value="101"/><column index="3" value="62"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">BEGIN TRANSACTION;

-- 1) Crear una tabla nueva sin 'apellido'
CREATE TABLE persona_new (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  nombre TEXT NOT NULL,
  profesion TEXT,
  nacionalidad TEXT,
  estudios TEXT
);

-- 2) Copiar los datos (ignora 'apellido')
INSERT INTO persona_new (id, nombre, profesion, nacionalidad, estudios)
SELECT id, nombre, profesion, nacionalidad, estudios
FROM persona;

-- 3) Reemplazar la tabla
DROP TABLE persona;
ALTER TABLE persona_new RENAME TO persona;

COMMIT;

